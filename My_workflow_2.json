    {
      "parameters": {
        "jsCode": "const rawAiOutput = items?.[0]?.json?.text || \"\";\n\nconst sanitizedOutput = rawAiOutput\n  .replace(/```json/gi, \"\")\n  .replace(/```/g, \"\")\n  .trim();\n\ntry {\n  const structuredPayload = JSON.parse(sanitizedOutput);\n\n  if (structuredPayload.reply_text === \"SKIP\") {\n      return [];\n  }\n\n  if (structuredPayload.reply_text) {\n      structuredPayload.reply_text = structuredPayload.reply_text\n          .replace(/\\\\n/g, \" \") \n          .replace(/[\\r\\n]+/gm, \" \")\n          .replace(/\\s+/g, \" \")  \n          .trim();\n  }\n\n  return structuredPayload;\n\n} catch (error) {\n  return {\n    category: \"General\",\n    priority: \"Medium\",\n    summary: \"Error\",\n    reply_text: \"Thank you for your email. We will review it shortly.\"\n  };\n}"
      },
